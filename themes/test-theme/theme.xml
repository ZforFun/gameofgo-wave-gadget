<?xml version="1.0" encoding="UTF-8" ?>
<theme>
  <name>Simple Board</name>
  <board>
    <image>
      <leftOffset>20</leftOffset>
      <topOffset>20</topOffset>
      <verticalGap>32</verticalGap>
      <horizontalGap>32</horizontalGap>
      <url size="19">board19.png</url>
      <url size="13">board13.png</url>
      <url size="9">board9.png</url>
    </image>
    <audioNotification type="boardChange">
      <audio type="audio/ogg" src="audio/boardChangeNotification.ogg"/>
      <audio type="audio/mp3" src="audio/boardChangeNotification.mp3"/>
      <audio type="audio/wav" src="audio/boardChangeNotification.wav"/>
    </audioNotification>
  </board>
  <stone>
    <width>24</width>
    <height>24</height>
    <url type="1">black.png</url>
    <url type="2">white.png</url>
    <url type="1-last">black-last.png</url>
    <url type="2-last">white-last.png</url>
    <url type="1-dead">black-dead.png</url>
    <url type="2-dead">white-dead.png</url>
    <url type="1-terr">black-terr.png</url>
    <url type="2-terr">white-terr.png</url>
    <url type="ko">ko.png</url>
    <url type="neutral">neutral.png</url>
  </stone>
</theme>