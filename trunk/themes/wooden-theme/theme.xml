<?xml version="1.0" encoding="UTF-8" ?>
<theme>
  <name>Wooden Board</name>
  <board>
    <size>19</size>
    <image>
      <url>board.png</url>
      <leftOffset>15</leftOffset>
      <topOffset>15</topOffset>
      <verticalGap>23</verticalGap>
      <horizontalGap>23</horizontalGap>
    </image>
    <audioNotification type="boardChange">
      <audio type="audio/ogg" src="audio/boardChangeNotification.ogg"/>
      <audio type="audio/mp3" src="audio/boardChangeNotification.mp3"/>
      <audio type="audio/wav" src="audio/boardChangeNotification.wav"/>
    </audioNotification>
  </board>
  <stone>
    <width>23</width>
    <height>23</height>
    <url type="1">black_mc.png</url>
    <url type="2">white_mc.png</url>
    <url type="1-last">black-last_mc.png</url>
    <url type="2-last">white-last_mc.png</url>
    <url type="1-dead">black-dead_mc.png</url>
    <url type="2-dead">white-dead_mc.png</url>
    <url type="1-terr">black-terr_mc.png</url>
    <url type="2-terr">white-terr_mc.png</url>
    <url type="ko">ko_mc.png</url>
  </stone>
</theme>