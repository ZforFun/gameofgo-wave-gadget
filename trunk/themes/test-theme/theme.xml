<?xml version="1.0" encoding="UTF-8" ?>
<theme>
  <name>Simple Board</name>
  <board>
    <size>19</size>
    <image>
      <url>board.png</url>
      <leftOffset>20</leftOffset>
      <topOffset>20</topOffset>
      <verticalGap>32</verticalGap>
      <horizontalGap>32</horizontalGap>
    </image>
  </board>
  <stone>
    <width>24</width>
    <height>24</height>
    <url type="1">black.png</url>
    <url type="2">white.png</url>
    <url type="1-last">black-last.png</url>
    <url type="2-last">white-last.png</url>
  </stone>
</theme>